{
  "languageserver": {
        "zls" : {
            "command": "zls",
            "filetypes": ["zig"]
        },
        "coc-clangd" : {
            "command": "coc-clangd",
            "filetypes": ["c, cc"],
            "arguments": [
                "-Wno-unused-variable",
                "-Wno-infinite-recursion",
                "-Werror=implicit-function-declaration",
                "-Wshadow",
                "-Wno-shadow-field-in-constructor-modified",
                "-Wno-shadow-ivar",
                "-Wuninitialized",
                "-Wunused-label",
                "-Wunused-lambda-capture"
            ]
        }
  }
}
